<p id="notice"><%= notice %></p>

<center><div class="category-title"> <%= link_to 'List an Event', new_event_path %></div></center></br>

  <tbody>
    <% @events.reverse.each do |event| %>
      <tr>
       <td><div class="time">Last updated <%= time_ago_in_words(event.created_at) %> ago </div></td>
        <td><%= link_to event_path(event) do %> 
        <div class="post-title"><%= event.title %> - <%= event.location %></div></br>
        <% end %> </td>
      </tr>
    <% end %>
  </tbody>
  <%= link_to 'Back', root_url %>