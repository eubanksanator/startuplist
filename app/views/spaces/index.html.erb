<p id="notice"><%= notice %></p>

<center><div class="category-title"> <%= link_to 'Post to Office Space', new_space_path %></div></center></br>

  <tbody>
    <% @spaces.reverse.each do |space| %>
      <tr>
       <td>
       <% if space.created_at < 0.days.ago  %>
         <div class="time-new"><%= "Today" %> </div>
         <% else %>
         <div class="time">Last updated <%= time_ago_in_words(space.created_at) %> ago </div>
         <% end %>
       </td>
        <td><%= link_to space_path(space) do %> 
        <div class="post-title"><%= space.title %> - <%= space.location %></div></br>
        <% end %> </td>
      </tr>
    <% end %>
  </tbody>
  <%= link_to 'Back', root_url %>