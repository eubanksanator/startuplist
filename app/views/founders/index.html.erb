<p id="notice"><%= notice %></p>

<center><div class="category-title"> <%= link_to 'Post to Founder Dating', new_founder_path %></div></center></br>

  <tbody>
    <% @founders.reverse.each do |founder| %>
      <tr>
       <td>
       <% if (founder.created_at) > (24.hours.ago) == true %>
         <div class="time-new"><%= "Today" %> </div>
         <% else %>
         <div class="time">Last updated <%= time_ago_in_words(founder.created_at) %> ago </div>
         <% end %>
         </td>
        <td><%= link_to founder_path(founder) do %> 
        <div class="post-title"><%= founder.title %></div></br>
        <% end %> </td>
      </tr>
    <% end %>
  </tbody>
  <%= link_to 'Back', root_url %>