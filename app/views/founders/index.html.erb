<p id="notice"><%= notice %></p>

<center><div class="category-title"> <%= link_to 'Founder Dating', new_founder_path %></div></center></br>

  <tbody>
    <% @founders.reverse.each do |founder| %>
      <tr>
       <td><div class="time">Last updated <%= time_ago_in_words(founder.created_at) %> ago </div></td>
        <td><%= link_to founder_path(founder) do %> 
        <div class="post-title"><%= founder.title %></div></br>
        <% end %> </td>
      </tr>
    <% end %>
  </tbody>