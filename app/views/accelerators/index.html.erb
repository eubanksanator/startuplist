<p id="notice"><%= notice %></p>

<center><div class="category-title"> <%= link_to 'Post Accelerator/Incubator', new_accelerator_path %></div></center></br>

  <tbody>
    <% @accelerators.reverse.each do |accelerator| %>
      <tr>
       <td><div class="time">Last updated <%= time_ago_in_words(accelerator.created_at) %> ago </div></td>
        <td><%= link_to accelerator_path(accelerator) do %> 
        <div class="post-title"><%= accelerator.title %> - <%= accelerator.location %></div></br>
        <% end %> </td>
      </tr>
    <% end %>
  </tbody>
  <%= link_to 'Back', root_url %>